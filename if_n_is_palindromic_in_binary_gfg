class Solution {
  public:
    int isPallindrome(long long int n) {
        vector<int> v;
        while(n!=0){
            int a=n&1;
            
            v.push_back(a);
           n= n>>1;
        }
        
        int low=0;
        int high=v.size()-1;
        while(low<=high){
            if(v[low]!=v[high])return 0;
            high--;
            low++;
        }
        return 1;
    }
};
